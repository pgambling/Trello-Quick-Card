{
  "name": "Add to Trello",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Quickly add a new card to any list.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "https://api.trello.com/"
  ]
}